   "source": [
    "rows = int(input().split()[0])\n",
    "colored = False\n",
    "\n",
    "for _ in range(rows):\n",
    "    row = input().split()\n",
    "    if (not colored) and (len(set(row)&{'C', 'M', 'Y'})>0):\n",
    "        colored = True\n",
    "\n",
    "if colored:\n",
    "    print(\"#Color\")\n",
    "else:\n",
    "    print(\"#Black&White\")"
   ]
